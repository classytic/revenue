{
  "name": "@classytic/revenue-manual",
  "version": "0.1.0",
  "description": "Manual payment provider for @classytic/revenue - Reference implementation",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "keywords": [
    "revenue",
    "payment",
    "manual",
    "provider",
    "cash",
    "bank-transfer",
    "mobile-money",
    "bkash",
    "nagad",
    "rocket",
    "typescript"
  ],
  "author": "Sadman Chowdhury",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/classytic/revenue.git"
  },
  "scripts": {
    "build": "tsup",
    "build:watch": "tsup --watch",
    "typecheck": "tsc --noEmit",
    "clean": "rimraf dist",
    "prepublishOnly": "npm run clean && npm run build"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "LICENSE",
    "README.md"
  ],
  "peerDependencies": {
    "@classytic/revenue": ">=1.0.0"
  },
  "devDependencies": {
    "rimraf": "^6.0.0",
    "tsup": "^8.3.0",
    "typescript": "^5.6.3"
  },
  "dependencies": {
    "nanoid": "^5.1.6"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
